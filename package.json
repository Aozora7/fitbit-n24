{
    "name": "fitbit-n24",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "dev": "vite --host",
        "format": "prettier --write --list-different ./src/**/*.{ts,tsx}",
        "lint": "eslint --ext .ts,.tsx ./src/",
        "build": "npm run lint && npm run format && tsc -b && vite build",
        "preview": "vite preview",
        "analyze": "tsx cli/analyze.ts",
        "analyze-period": "tsx cli/analyze_period.ts",
        "split-gaps": "tsx cli/split-gaps.ts",
        "compare": "tsx cli/compare.ts",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "d3-scale": "^4.0.2",
        "d3-time-format": "^4.1.0",
        "lucide-react": "^0.574.0",
        "react": "^19.2.4",
        "react-dom": "^19.2.4",
        "swr": "^2.3.8"
    },
    "devDependencies": {
        "@eslint/js": "^9.0.0",
        "@tailwindcss/vite": "^4.1.18",
        "@types/d3-scale": "^4.0.9",
        "@types/d3-time-format": "^4.0.3",
        "@types/node": "^25.2.3",
        "@types/react": "^19.2.10",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.2",
        "eslint": "^9.0.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-react-hooks": "^7.0.1",
        "prettier": "^3.8.1",
        "tailwindcss": "^4.1.18",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.55.0",
        "vite": "^7.3.1",
        "vitest": "^4.0.18"
    }
}
